{#
/**
 * @file
 * CivicTheme theme implementation for main view template.
 */
#}
{% extends '@organisms/list/list.twig' %}

{% block footer %}
  {% if attachment_after is defined and attachment_after is not empty %}
    <div class="ct-list__attachment_after">
      <div class="container">
        <div class="row">
          <div class="col-xs-12">
            {{ attachment_after }}
          </div>
        </div>
      </div>
    </div>
  {% endif %}

  {% if more is defined and more is not empty %}
    <div class="ct-list__more">
      <div class="container">
        <div class="row">
          <div class="col-xs-12">
            {{ more }}
          </div>
        </div>
      </div>
    </div>
  {% endif %}

  {% if footer %}
    <footer class="ct-list__footer">
      {{ footer }}
    </footer>
  {% endif %}

  {% if feed_icons is defined and feed_icons is not empty %}
    <div class="ct-list__feed_icons">
      <div class="container">
        <div class="row">
          <div class="col-xs-12">
            {{ feed_icons }}
          </div>
        </div>
      </div>
    </div>
  {% endif %}
{% endblock %}
