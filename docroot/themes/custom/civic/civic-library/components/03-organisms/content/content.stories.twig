  } only %}

  <div class="civic-block">
    {% include '@atoms/heading/heading.twig' with {
      level: 3,
      title: "This is Promo card example",
    } only %}
    <div class="row">
      {% for i in 1..4  %}
        <div class="col-m-3">
          {% include '@molecules/promo-card/promo-card.twig' %}
        </div>
      {% endfor %}
    </div>
  </div>

  <div class="civic-block">
    {% include '@atoms/heading/heading.twig' with {
      level: 3,
      title: "This is Events card example",
    } only %}
