{
  "name": "civic",
  "version": "0.1.0",
  "description": "Civic components with Storybook integration.",
  "keywords": [
    "components",
    "storybook"
  ],
  "homepage": "https://github.com/salsadigitalauorg/civic-node",
  "bugs": {
    "url": "https://github.com/salsadigitalauorg/civic-node/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "build-storybook --quiet",
    "storybook": "start-storybook",
    "storybook-css": "webpack -d --config ./webpack/webpack.config.js --display-error-details",
    "storybook-js": "npx babel components -d dist/components --ignore 'components/**/*.stories.js' --presets=@babel/preset-env,babel-preset-minify --plugins=babel-plugin-drupal-behaviors",
    "auto": "auto",
    "sass": "node-sass --output-style expanded components -o components",
    "sass-watch": "node-sass -w --output-style expanded components -o components",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.0",
    "@storybook/addon-knobs": "^6.3.0",
    "@storybook/html": "^6.3.6",
    "auto": "^7.11.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-drupal-behaviors": "^0.1.1",
    "babel-preset-minify": "^0.5.1",
    "drupal-attribute": "^1.0.2",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^6.0.1",
    "twig": "^1.15.4",
    "twig-drupal-filters": "^3.1.2",
    "twig-loader": "^0.5.1",
    "twigjs-loader": "^1.0.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-fix-style-only-entries": "^0.4.0"
  }
}
