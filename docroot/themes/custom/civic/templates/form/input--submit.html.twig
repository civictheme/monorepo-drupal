{% if 'secondary' in attributes.class|join(' ') %}
  {% set type = 'secondary' %}
{% else %}
  {% set type = 'primary' %}
{% endif %}
{% set children %}
  {% include "@atoms/button/button.twig" with {
    type: type,
    text: attributes.value,
    modifier_class: attributes.class,
    attributes: attributes|without('class')
  } only %}
{% endset %}
{% include "@organisms/form-element/form-element.twig" with {
  type: 'submit',
  children: children,
} only %}

