    "webpack-merge": "^5.3.0"
  },
  "scripts": {
    "build": "npm run gulp:build && npm run webpack:build && build-storybook -s ./dist,./images -o .storybook-static",
    "storybook": "npm run gulp:build && concurrently --raw \"npm run gulp:watch\" \"npm run webpack:watch\" \"start-storybook --ci -s ./dist,./images -p 6006\"",
    "a11y": "npm run build && ./scripts/a11y.js -r",
    "test": "npm run gulp:build && jest --coverage",
    "twatch": "npm run gulp:build && jest --no-coverage --watch --verbose",
    "coverage": "npm run test && open-cli .coverage/lcov-report/index.html",
    "lint": "eslint ./components",
    "postinstall": "npx patch-package",
    "gulp:build": "gulp build",
    "gulp:watch": "gulp watch",
    "webpack:watch": "webpack --watch --config ./webpack/webpack.dev.js",
    "webpack:build": "webpack --config ./webpack/webpack.prod.js"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.1.18",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/addons": "^6.1.18",
    "@storybook/html": "^6.1.18",
    "@storybook/storybook-deployer": "^2.8.7",
    "babel-jest": "^26.6.3",
    "chalk": "^4.0.0",
    "core-js": "3.9.0",
    "del": "^4.1.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-flatmap": "^1.0.2",
    "gulp-newer": "^1.4.0",
    "gulp-rename": "^2.0.0",
    "jest": "^26.6.3",
    "open-cli": "^6.0.1",
    "pa11y": "^5.3.0",
    "twig-testing-library": "^1.1.1",
    "yaml": "^1.10.0"
  }
}
