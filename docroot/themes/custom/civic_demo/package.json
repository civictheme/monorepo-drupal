{
  "name": "civic-demo-theme",
  "version": "1.0.0",
  "private": true,
  "description": "Components for Civic Drupal theme",
  "scripts": {
    "build": "npm run dist && build-storybook --quiet",
    "build-storybook": "npm run gulp:build && build-storybook",
    "storybook": "npm run gulp:build && concurrently \"npm run gulp:watch\" \"start-storybook -s ./assets\"\n",
    "gulp:build": "gulp",
    "gulp:watch": "gulp watch",
    "lint": "eslint ./components ./assets/js",
    "dist": "npm run gulp:build && webpack --config ./webpack/webpack.prod.js",
    "dist:dev": "npm run gulp:build && webpack --config ./webpack/webpack.dev.js",
    "generate-icons": "node ./scripts/generate-icons.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@storybook/addon-a11y": "^6.1.18",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addons": "^6.1.18",
    "@storybook/html": "^6.3.8",
    "@storybook/preset-scss": "^1.0.3",
    "@whitespace/storybook-addon-html": "^5.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-drupal-behaviors": "^0.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.2.1",
    "css-loader": "^5.2.7",
    "drupal-attribute": "^1.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "gulp": "^4.0.2",
    "gulp-flatmap": "^1.0.2",
    "gulp-newer": "^1.4.0",
    "gulp-rename": "^2.0.0",
    "html-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass-glob-importer": "^5.3.2",
    "sass": "^1.41.0",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "svg-sprite-loader": "^6.0.9",
    "twig": "^1.15.4",
    "twig-drupal-filters": "^3.1.2",
    "twigjs-loader": "^1.0.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "breakpoint-sass": "^2.7.1",
    "normalize.css": "^8.0.1"
  }
}
