{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Id for content"
      },
      "description": {
        "type": "string",
        "description": "Block machine name"
      },
      "content": {
        "type": "array",
        "items": {
          "type": "string",
          "description": "Block content uuid"
        }
      },
      "region": {
        "type": "string",
        "description": "Region"
      }
    },
    "required": [
      "id",
      "description",
      "content",
      "region"
    ]
  }
}
