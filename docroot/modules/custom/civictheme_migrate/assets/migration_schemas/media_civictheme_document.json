  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "uuid": {
        "type": "string",
        "description": "Entity UUID"
      },
      "name": {
        "type": "string",
        "description": "Entity name"
      },
      "file": {
        "type": "string",
