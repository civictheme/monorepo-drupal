{#
/**
 * @file
 * Theme implementation to display a styleguide item.
 */
#}
<a name="{{ key }}"></a>
<div{{ attributes }}>
  <h2 class="styleguide">{{ item.title }}</h2>
  {% if item.description %}
    <p class="styleguide-description">
      {{ item.description }}
    </p>
    <hr/>
  {% endif %}
  <div class="styleguide">
    {{ item.before_content }}
    {{ content }}
    {{ item.after_content }}
  </div>
</div>
<hr/>
