{#
/**
 * @file
 * CivicTheme implementation to display a 'submit' form element.
 */
#}

{% if 'secondary' in modifier_class %}
  {% set type = 'secondary' %}
{% else %}
  {% set type = 'primary' %}
{% endif %}

{% include "@atoms/button/button.twig" with {
  type: type,
  text: value,
  modifier_class: modifier_class,
  attributes: attributes,
  kind: 'submit',
  theme: theme
} only %}
